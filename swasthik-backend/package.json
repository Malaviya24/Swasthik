{
  "name": "swasthik-backend",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_ENV=development && npx tsx index.ts",
    "dev:unix": "NODE_ENV=development npx tsx index.ts",
    "build": "esbuild index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "set NODE_ENV=production && node dist/index.js",
    "start:unix": "NODE_ENV=production node dist/index.js",
    "check": "tsc"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@neondatabase/serverless": "^0.10.4",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/multer": "^2.0.0",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  }
}